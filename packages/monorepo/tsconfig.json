{
	"extends": "./../../tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"rootDir": "./src",
		"skipLibCheck": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noImplicitAny": false,
		"strict": false,
		"strictNullChecks": true,
		"noEmit": true,
		"paths": {
			"effect": ["./src/effect-wrapper.ts"]
		}
	},
	"exclude": ["node_modules", "dist"],
	"include": ["package.json", "src"]
}
