{
	"name": "@wts/error",
	"version": "0.1.0",
	"private": true,
	"description": "Type-safe error handling with Result<T, E> - Functional programming approach",
	"keywords": [
		"error",
		"error-handling",
		"result",
		"functional",
		"type-safe"
	],
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"dev": "tsdown --watch",
		"build": "tsdown --exports --dts --minify",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit",
		"lint": "tsc --noEmit && oxlint --fix",
		"review": "bun run lint && bun run test && bun run build"
	},
	"dependencies": {
		"@effect/schema": "^0.75.5",
		"effect": "^3.19.13",
		"@wts/functional": "workspace:*"
	},
	"devDependencies": {
		"@types/bun": "^1.3.5",
		"@types/node": "^20.19.27",
		"oxlint": "^0.17.0",
		"tsdown": "^0.16.8",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"module": "./dist/index.mjs"
}
