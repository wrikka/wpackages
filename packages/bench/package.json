{
	"name": "bench",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"description": "Benchmarking utility for WTS packages with functional programming patterns",
	"keywords": [
		"benchmark",
		"performance",
		"testing",
		"typescript",
		"functional"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsdown --exports --dts --minify",
		"dev": "tsdown --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "tsc --noEmit && oxlint --fix",
		"review": "bun run format && bun run lint && bun run test && bun run build"
	},
	"dependencies": {
		"@effect/schema": "^0.75.5",
		"effect": "^3.19.13",
		"picocolors": "^1.1.1"
	},
	"devDependencies": {
		"@types/bun": "^1.3.5",
		"@types/node": "^24.10.4",
		"execa": "^9.6.1",
		"fast-check": "^3.23.2",
		"tsdown": "^0.15.12",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"module": "./dist/index.js"
}
