{
  "name": "cli-builder",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "watch": "bun lint && bun watch && bun test",
    "start": "bun --watch dev && vitest:ui",
    "dev": "bun run src/index.ts",
    "lint": "tsc --noEmit && biome lint --write",
    "build": "tsdown",
    "format": "biome format --write",
    "test": "vitest --run",
    "verify": "bun format && bun lint && bun test && bun audit && run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsdown": "^0.18.3",
    "vitest": "^4.0.16",
    "oxlint": "latest",
    "oxlint-tsgolint": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "c12": "^3.3.3"
  },
  "packageManager": "bun@1.3.5"
}
