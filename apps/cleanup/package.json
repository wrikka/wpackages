    "rimraf": "^6.1.2"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/glob": "^8.1.0",
    "@types/node": "^24.10.4",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
