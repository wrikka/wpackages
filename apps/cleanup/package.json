{
  "name": "computer-cleanup",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "bin": {
    "computer-cleanup": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsdown src/bin/cli.ts --out-dir dist --dts --minify",
