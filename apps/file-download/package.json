{
	"name": "@wpackages/file-download",
	"version": "1.0.0",
	"description": "A CLI tool for development tasks",
	"type": "module",
	"bin": "./dist/index.js",
	"scripts": {
		"watch": "bun lint && bun watch && bun test",
		"start": "bun --watch dev && vitest:ui",
		"dev": "bun run src/index.ts",
		"lint": "tsc --noEmit && biome lint --write",
		"build": "tsdown",
		"format": "biome format --write",
		"test": "vitest --run",
		"verify": "bun format && bun lint && bun test && bun audit && run build"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"axios": "^1.13.2",
		"commander": "14.0.2",
		"picocolors": "^1.1.1",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"oxlint": "^1.35.0",
		"tsdown": "^0.18.3",
		"typescript": "^5.9.3"
	},
	"author": "",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": ""
	},
	"packageManager": "bun@1.3.5"
}
