{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"incremental": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"noEmit": true,
		"skipLibCheck": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strict": false,
		"strictNullChecks": true
	},
	"exclude": [
		"node_modules",
		"dist"
	],
	"include": [
		"src"
	]
}
