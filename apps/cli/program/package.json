{
	"name": "@wpackages/program",
	"packageManager": "bun@1.3.8",
	"type": "module",
	"scripts": {
		"watch": "bun run dev",
		"start": "bun run dev",
		"dev": "bun run src/index.ts",
		"format": "dprint fmt",
		"lint": "oxlint --fix --type-aware",
		"build": "tsdown",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"verify": "bun format && bun lint && bun run test && (bun audit || true) && bun run build"
	},
	"devDependencies": {
		"tsdown": "^0.18.4",
		"vite-tsconfig-paths": "^6.0.5"
	},
	"dependencies": {
		"@wpackages/config-manager": "workspace:*",
		"@wpackages/logger": "workspace:*",
		"@wpackages/observability": "workspace:*",
		"effect": "^3.19.15"
	}
}
