{
	"bin": {
		"wcheck": "./dist/bin/cli.js",
		"wts-check": "./dist/bin/cli.js"
	},
	"dependencies": {
		"@clack/prompts": "^1.0.0",
		"@wpackages/unused": "workspace:*",
		"cac": "^6.7.14",
		"chalk": "^5.6.2",
		"effect": "^3.19.15",
		"picocolors": "^1.1.1",
		"shiki": "^3.21.0",
		"ts-morph": "^27.0.2",
		"typescript": "^5.9.3"
	},
	"description": "Comprehensive code quality checker with TypeScript, unused code, dependencies, and more - Built with Effect-TS",
	"devDependencies": {
		"@types/bun": "^1.3.8",
		"@types/node": "^25.1.0",
		"dprint": "^0.51.1",
		"knip": "^5.82.1",
		"oxlint": "^1.42.0",
		"oxlint-tsgolint": "^0.11.3",
		"tsdown": "^0.20.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"keywords": [
		"cli",
		"checker",
		"type-check",
		"unused-code",
		"dependencies",
		"code-quality",
		"effect-ts",
		"functional",
		"typescript",
		"linter"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"name": "@wpackages/check",
	"scripts": {
		"watch": "bun lint && bun watch && bun test",
		"start": "bun --watch dev && vitest:ui",
		"dev": "bun run src/check/index.ts",
		"lint": "tsc --noEmit && oxlint --fix --type-aware",
		"build": "tsdown",
		"format": "dprint fmt",
		"test": "vitest --run --passWithNoTests",
		"verify": "bun format && bun lint && bun test && bun audit && run build"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "0.1.0",
	"packageManager": "bun@1.3.8"
}
