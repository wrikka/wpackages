{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "types": [
      "node"
    ],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true  },
  "include": ["src"]
}
