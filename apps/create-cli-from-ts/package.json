{
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "cli-builder": "workspace:*",
    "openai": "^6.15.0"
  },
  "description": "Complete CLI toolkit - Commands, Components, and Prompts",
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.3",
    "dprint": "^0.50.2",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.10.0",
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "keywords": [
    "cli",
    "command",
    "prompt",
    "components",
    "terminal",
    "cli-components",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "create-cli-from-ts": "./dist/index.js"
  },
  "name": "create-cli-from-ts",
  "scripts": {
    "watch": "bun lint && bun watch && bun test",
    "start": "bun --watch dev && vitest:ui",
    "dev": "bun run src/index.ts",
    "examples": "bun run src/index.ts build --entry examples/hello.ts --name example-generated-cli",
    "lint": "oxlint --fix",
    "postbuild": "bun run src/scripts/add-shebang.ts",
    "build": "tsdown && bun run postbuild",
    "format": "dprint fmt",
    "test": "vitest --run --passWithNoTests",
    "verify": "bun format && bun lint && bun run test && bun audit && bun run build"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.1",
  "packageManager": "bun@1.3.5"
}
