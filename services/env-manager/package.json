{
	"name": "env-manager",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"bin": {
		"wenv": "./dist/cli.js"
	},
	"main": "./dist/cli.js",
	"module": "./dist/cli.js",
	"types": "./dist/cli.d.ts",
	"exports": {
		".": "./dist/cli.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"build": "tsdown --exports --dts --minify",
		"lint": "tsc --noEmit && oxlint --fix",
		"test": "vitest run",
		"verify": "bun run lint && bun run build && bun run test"
	},
	"dependencies": {
		"@wts/config-manager": "workspace:*"
	},
	"devDependencies": {
		"@types/bun": "1.3.5",
		"@types/node": "^20.19.27",
		"oxlint": "^1.35.0",
		"tsdown": "^0.18.3",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"packageManager": "bun@1.3.5"
}
