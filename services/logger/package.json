{
	"name": "@wpackages/logger",
	"version": "0.1.0",
	"description": "Logger service (Effect tag) with levels and redaction",
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	},
	"files": ["dist", "README.md"],
	"scripts": {
		"build": "tsdown --exports --dts --minify",
		"dev": "bun --watch src/index.ts",
		"format": "dprint fmt",
		"lint": "oxlint --fix --type-aware",
		"test": "vitest run",
		"verify": "bun run format && bun run lint && (bun audit || true) && bun run build"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/bun": "^1.3.5",
		"@types/node": "^20.19.27",
		"effect": "^3.19.14",
		"tsdown": "^0.18.4",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"effect": "^3.19.14"
	},
	"packageManager": "bun@1.3.5",
	"engines": {
		"bun": ">=1.3.5"
	}
}
