{
  "name": "@wpackages/httpify",
  "version": "0.0.1",
  "private": true,
  "description": "HTTP framework built for high performance and portability.",
  "homepage": "https://github.com/newkub/wpackages",
  "license": "MIT",
  "repository": "newkub/wpackages",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir ./dist",
    "lint": "tsc --noEmit && oxlint --fix --type-aware",
    "format": "dprint fmt",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "watch": "bun --watch src/index.ts",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "rou3": "^0.7.12",
    "srvx": "^0.11.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "bun-types": "latest",
    "dprint": "^0.47.0",
    "oxlint": "^1.43.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "bun": ">= 1.0.0"
  },
  "keywords": [
    "http",
    "framework",
    "server",
    "api",
    "web",
    "bun",
    "typescript"
  ]
}
