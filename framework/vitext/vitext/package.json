{
	"name": "vitext",
	"version": "0.0.1",
	"private": true,
	"description": "A modern, fast, and lightweight web framework for building web applications, built to be better than Vite.",
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"build": "tsdown --exports --dts --minify",
		"server": "bun src/server.ts",
		"test": "vitest",
		"lint": "tsc --noEmit && oxlint --fix",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"radix3": "^1.1.2"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"tsdown": "^0.17.2",
		"typescript": "^5.9.3",
		"vitest": "4.0.9"
	},
	"peerDependencies": {
		"typescript": ">=5.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	}
}
