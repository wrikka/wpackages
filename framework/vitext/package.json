{
  "name": "@wrikka/vite-plugins",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "test": "turbo test",
    "update:deps": "taze -r -w && bun install",
    "review": "bun update:deps && bun format && bun lint && bun test && bun build",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "lefthook": "^2.0.13",
    "taze": "latest",
    "tsdown": "^0.17.3",
    "turbo": "latest",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
